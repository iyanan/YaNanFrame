<?xml version="1.0" encoding="UTF-8"?>
<Token>
	<!-- 
		token子类用于验证客户端是否有权访问某个命名空间
		namespace 要求token验证的命名空间
		role       开放访问的角色
		chain     无须验证的链接
		command中redirect 重定向，目前也只支持重定向命令 值是重定向的地址
	 -->
	 <Config 
		TokenMark="TUID"
		RoleMark="ROLES"
		Timeout="86400"
	></Config>
	<listener namespace="/*" class="com.YaNan.frame.hibernate.configure.defaultServletListener" />
</Token>